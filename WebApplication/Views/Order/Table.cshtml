@model List<Order>;
@{
    ViewData["Title"] = "Table";
    Layout = "_AdminPanel";
}

<table  class="table table-hover" style="margin-top: 150px;">
    <thead>
        <tr>
            <th>Id</th>
            <th>Статус</th>
            <th>Дата</th>
            <th>Имя</th>
            <th>Фамилия</th>
            <th>Регион</th>
            <th>Город</th>
            <th>Улица</th>
            <th>ДОм/Квартира</th>
            <th>Номер телефона</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.Id</td>
                <td>@order.Status</td>
                <td>@order.Date</td>
                <td>@order.Customer.Name</td>
                <td>@order.Customer.Surname</td>
                <td>@order.Customer.Address.Region</td>
                <td>@order.Customer.Address.City</td>
                <td>@order.Customer.Address.Street</td>
                <td>@order.Customer.Address.HouseOrFlat</td>
                <td>@order.Customer.PhoneNumber</td>
                <td><a href="#">Изменить</a></td>
            </tr>

        }
    </tbody>
</table>
