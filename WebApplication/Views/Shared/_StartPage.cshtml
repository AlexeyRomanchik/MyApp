        $(function () {
            $('.searchbutton').bind('click',
                function () {

                    if (window.SpeechRecognition || window.webkitSpeechRecognition || window.mozSpeechRecognition || window.msSpeechRecognition) {
                        console.log('Браузер поддерживает данную технологию');
                    } else {
                        console.log('Не поддерживается данным браузером');
                    }

                    var SpeechRecognition = new window.webkitSpeechRecognition();

                    var SpeechRecognition = new window.webkitSpeechRecognition();
                    SpeechRecognition.lang = "ru-RU";
                    SpeechRecognition.onresult = function (event) {
                        console.log(event.results[0][0].transcript);
                        var searchText = event.results[0][0].transcript;

                        var request = new XMLHttpRequest();

                        function reqReadyStateChange() {
                            if (request.readyState == 4) {
                                var status = request.status;
                                if (status == 200) {
                                    document.write(request.responseText);
                                } else {
                                    document.write(request.responseText);
                                }
                            }
                        }

                        request.open("GET", "/Ram/Index?name=" + searchText, true);
                        request.onreadystatechange = reqReadyStateChange;
                        request.send();

                    };
                    SpeechRecognition.onend = function () {
                        SpeechRecognition.start();
                    };
                    SpeechRecognition.start();
                });
        });


